
// print if the sum is even or odd
#include <iostream>
using namespace std;

int main() {
    int x1,x2;
    int sum;
    cout<<"Enter 2 numbers:"<<endl;
    cin>>x1>>x2;
    sum=x1+x2;
    if(sum%2==0)
    {
        cout<<" Sum is Even "<<endl;
    }
    else{
        cout<<"Sum is odd";
    }
    
    return 0;
}
